# Directives for unison
ignore = Name .Xauthority
ignore = Name lost+found
ignore = Name *socket

auto   = true
batch  = true
confirmbigdel = true

group  = true
owner  = true
times  = true

prefer = newer
silent = true

sshargs = -i /root/.ssh/swarm-sync-sshkey.rsa
logfile = /var/log/unison/unison.log
